@model List<Person>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
      <table class="table">
          <thead>
            <tr>
              <th scope="col">Id</th>
              <th scope="col">Last Name</th>
              <th scope="col">First Name</th>
              <th scope="col">Middle Name</th>
            </tr>
          </thead>
          <tbody>
            @foreach (var p in Model)
            {
              <tr>
                <th scope="row">@p.Id</th>
                <td>@p.LastName</td>
                <td>@p.FirstName</td>
                <td>@p.MiddleName</td>
              </tr>
            }
          </tbody>
      </table>
</div>